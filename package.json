{
  "name": "highfive.js",
  "version": "0.17.1",
  "description": "html5/js game framework",
  "main": "example/helloworld/main.js",
  "directories": {
    "dist": "dist",
    "example": "example",
    "src": "src",
    "test": "test",
    "tool": "tool"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raphaelstary/highfive.js.git"
  },
  "keywords": [
    "html5",
    "js",
    "game",
    "framework"
  ],
  "author": "Raphael Stary",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raphaelstary/highfive.js/issues"
  },
  "homepage": "https://github.com/raphaelstary/highfive.js",
  "devDependencies": {
    "uglify-js": "^2.8.17"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    // build scripts:
    "clean": "node tool/clean.js",
    "init": "node --eval \"fs.mkdirSync('dist');\"",
    "gen:html": "node tool/generateHtml.js",
    "concat": "node tool/concatenate.js",
    "minify": "uglifyjs dist/highfive.js > dist/h5-min.js",
    // all build scripts in sequence:
    "build": "npm run clean && npm run init && npm run gen:html && npm run concat && npm run minify",
    // npm events:
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "postversion": "git push && git push --tags"
  }
}
